{
  "name": "vuex-webextensions-example",
  "description": "A webextension example for Vuex WebExtensions",
  "version": "1.0.0",
  "author": "Mitsuha Kitsune <contact@mitsuhakitsune.com> (https://mitsuhakitsune.com/)",
  "license": "MIT",
  "homepage": "https://github.com/MitsuhaKitsune/vuex-webextensions#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MitsuhaKitsune/vuex-webextensions.git"
  },
  "bugs": {
    "url": "https://github.com/MitsuhaKitsune/vuex-webextensions/issues"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "precommit": "pretty-quick --staged",
    "prettier": "prettier 'src/**/*.{js,vue}'",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "dependencies": {
    "vue": "^3.1.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime-corejs3": "^7.4.0",
    "@vue/compiler-sfc": "^3.1.5",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.4.0",
    "ejs": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-loader": "^16.4.1",
    "vue-template-compiler": "^2.6.14",
    "web-ext-types": "^2.1.0",
    "webextension-polyfill": "^0.3.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "webpack-extension-reloader": "^1.1.0"
  }
}
